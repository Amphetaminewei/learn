        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library"
        mc:Ignorable="d"
        Title="请输入要归还书的编号" Height="200" Width="300">
    <Grid>
        <Label Content="请输入要归还书的编号：" HorizontalAlignment="Left" Margin="70,31,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="93,61,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="booknumber"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="70,107,0,0" VerticalAlignment="Top" Width="75" Click="clickReturn"/>
        <Button Content="取消" HorizontalAlignment="Left" Margin="190,107,0,0" VerticalAlignment="Top" Width="75" Click="Cancle"/>

    </Grid>
</Window>
